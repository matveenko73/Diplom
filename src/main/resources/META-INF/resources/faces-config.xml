            <from-outcome>goToHello2</from-outcome>
            <to-view-id>/hello2</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>goToProfile</from-outcome>
            <to-view-id>/Profile</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>goToPictures</from-outcome>
            <to-view-id>/Pictures</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>goToHistory</from-outcome>
            <to-view-id>/History</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>goToAbout</from-outcome>
            <to-view-id>/About</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>goToMain</from-outcome>
            <to-view-id>/Main</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>goToMessages</from-outcome>
            <to-view-id>/Messages</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>goToSecurity</from-outcome>
